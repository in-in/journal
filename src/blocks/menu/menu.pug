block append templates
  template#main-menu
    //- nav(role='navigation').menu
    nav(role='navigation', v-if="menuShow").menu
      ul.menu__list
        li(
            v-for='(i, index) in items',
            :style='{order: index + 1}'
          ).menu__item
          a(href='#').menu__link {{ i[0] }}
      p foo + {{ message }}
